[alias]
	alias =  !git config --list | grep '^alias\\.'
        branch-rm = !git branch --merged | grep -vE '^\\*|\\<master\\>|\\<develop\\>' | xargs -I % git branch -d %
        branch-rmf = !git branch | grep -vE '^\\*|\\<master\\>|\\<develop\\>' | xargs -I % git branch -D %
        close = !CURRENT_BRANCH=$(git rev-parse --abbrev-ref HEAD) && git checkout master && git pull && git branch -D ${CURRENT_BRANCH}


